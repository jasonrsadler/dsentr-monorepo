  "query": "\n            UPDATE user_oauth_tokens\n            SET is_shared = $3, updated_at = now()\n            WHERE user_id = $1 AND provider = $2::oauth_connection_provider\n            RETURNING\n                id,\n                user_id,\n                provider as \"provider: _\",\n                access_token,\n                refresh_token,\n                expires_at,\n                account_email,\n                is_shared,\n                created_at,\n                updated_at\n            ",
